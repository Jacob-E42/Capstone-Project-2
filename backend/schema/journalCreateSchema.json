{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://example.com/journalEntry.json",
	"type": "object",
	"properties": {
		"userId": {
			"type": "integer",
			"minimum": 1
		},
		"title": {
			"type": "string",
			"minLength": 1
		},
		"entryText": {
			"type": "string",
			"minLength": 1
		},
		"entryDate": {
			"type": "string",
			"format": "date"
		},
		"emotions": {
			"type": "object",
			"additionalProperties": {
				"type": "string"
			}
		}
	},
	"required": ["title", "entryText", "entryDate"],
	"additionalProperties": false
}
